<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
    <settings defaultResolver="private"	defaultConflictManager="all"/>
    <resolvers>
        <url name="private">
            <ivy pattern="${ivy.src.repo}/[organisation]/[module]/[revision]/ivy.xml"/>
            <artifact pattern="${ivy.src.repo}/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
            <artifact pattern="${ivy.src.repo}/[organisation]/[module]/[revision]/[artifact].[ext]"/>
        </url>
    </resolvers>
</ivysettings>
